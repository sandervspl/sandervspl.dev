{
  "name": "sandervspl.dev",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@10.11.0",
  "targets": {
    "react-static": {
      "source": "src/pages/*",
      "context": "react-server"
    }
  },
  "scripts": {
    "dev": "rm -rf dist && NODE_ENV=development parcel",
    "build": "NODE_ENV=production parcel build && cp -r public/* dist/",
    "build:cloudflare": "pnpm run build",
    "prebuild": "pnpm test",
    "format": "biome format . --write",
    "lint": "biome lint scripts/",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@parcel/config-default": "2.15.2",
    "@parcel/packager-react-static": "2.15.2",
    "@parcel/rsc": "2.15.2",
    "@parcel/transformer-react-static": "2.15.2",
    "@parcel/transformer-svg-jsx": "2.15.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^26.1.0",
    "parcel": "2.15.2",
    "postcss": "^8.5.6",
    "react-server-dom-parcel": "^19.1.0",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.11",
    "vitest": "^3.2.4"
  }
}
